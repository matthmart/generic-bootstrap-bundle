<!DOCTYPE html>
<html>
<head>
    <title>Bootstrap Test</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Bootstrap -->
    {% stylesheets 
        'bundles/eutechgenericbootstrap/twbs/css/bootstrap.min.css' 
        'bundles/eutechgenericbootstrap/css/layout.css'
    filter='cssrewrite' %}
        <link href="{{ asset_url }}" rel="stylesheet" media="screen">
    {% endstylesheets %}

</head>
<body>
    {% block navbar %}
        <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Afficher/masquer</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">{% block navbar_brand %}Brand{% endblock %}</a>
            </div>

            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav">
                {% block navbar_links %}
                    <li><a href="#">Link</a></li>
                {% endblock %}
                </ul>
            </div>
        </nav>
    {% endblock %}

    {% block content %}{% endblock %}

    <!-- jQuery and Bootstrap JS file -->
    {% javascripts 
        '@EutechGenericBootstrapBundle/Resources/public/jquery/jquery.min.js' 
        '@EutechGenericBootstrapBundle/Resources/public/twbs/js/bootstrap.min.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
</body>
</html>